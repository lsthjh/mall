@import "./styles/iconfont.wxss";
@import "./weui/weui.wxss";

/* 微信小程序不支持 通配符（*） */
page,
view,
text,
swiper,
swiper-item,
image,
navigator {
  padding: 0;
  margin: 0;
  box-sizing: border-box;
}

/* 定义主题 */
page{
  /* 主题颜色 */
  --themeColor:#02a871;
  /* 统一文字大小 */
  font-size: 28 rpx;
}